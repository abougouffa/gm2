(*
   PushRememberConstant - 
*)

PROCEDURE PushRememberConstant ;


(*
   PopRememberConstant - 
*)

PROCEDURE PopRememberConstant ;


(*
   RememberConstant - 
*)

PROCEDURE RememberConstant (sym: CARDINAL) ;


(*
   FixupConstAsString - fixes up a constant, sym, which will have the string type.
*)

